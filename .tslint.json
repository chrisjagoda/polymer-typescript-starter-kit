{
  "extends": "tslint:latest",
  "rules": {
    "arrow-parens": [true, "ban-single-arg-parens"],
    "member-access": false,
    "no-console": false,
    "no-reference": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": false,
    "variable-name": {
      "check-format": "allow-leading-underscore"
    }
  }
}
